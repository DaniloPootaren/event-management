{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "date": {
      "type": "datetime"
    },
    "banner": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "hosted_by": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "development conf",
        "leisure",
        "hicking",
        "forest",
        "swimming ",
        "bicycle ",
        "dirt bike",
        "quad"
      ]
    },
    "event_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event-detail.event-detail"
    }
  }
}
