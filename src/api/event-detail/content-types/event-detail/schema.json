{
  "kind": "collectionType",
  "collectionName": "event_details",
  "info": {
    "singularName": "event-detail",
    "pluralName": "event-details",
    "displayName": "event-detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "event": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::event.event",
      "mappedBy": "event_detail"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "SHOWING",
        "UPCOMING",
        "EXPIRED"
      ],
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "organizer": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "text",
      "required": true
    },
    "phonenumber": {
      "type": "integer",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "fax": {
      "type": "string",
      "required": true
    },
    "network": {
      "type": "json",
      "required": true
    },
    "speakers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::speaker.speaker",
      "mappedBy": "event_detail"
    },
    "gallery": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
